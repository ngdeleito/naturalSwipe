<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>naturalSwipe@ngdeleito.eu</em:id>
    <em:version>1.0</em:version>
    <em:type>2</em:type>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>20.0</em:minVersion>
        <em:maxVersion>20.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:bootstrap>true</em:bootstrap>
    <em:multiprocessCompatible>true</em:multiprocessCompatible>
    
    <em:name>Natural Swipe</em:name>
    <em:description>Swipe gestures with three fingers consistent with natural scrolling.</em:description>
    <em:creator>Nicolás González-Deleito</em:creator>
    <em:homepageURL>https://github.com/ngdeleito/naturalSwipe</em:homepageURL>

    <em:localized>
